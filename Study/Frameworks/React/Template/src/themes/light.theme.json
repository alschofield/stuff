{
  "nav": {
    "container": {
      "position": "fixed",
      "display": "flex",
      "backgroundColor": "#967bb6",
      "width": "calc(100% - 20px)",
      "height": "40px",
      "top": "0px",
      "left": "0px",
      "zIndex": "1",
      "alignItems": "center",
      "justifyContent": "space-between",
      "paddingLeft": "10px",
      "paddingRight": "10px"
    },
    "item": {
      "height": "34px",
      "color": "#111111"
    },
    "icon": {
      "width": "100%",
      "height": "90%",
      "cursor": "pointer"
    }
  },
  "home": {
    "container": {
      "display": "block",
      "height": "calc(100vh - 115px)",
      "paddingTop": "40px",
      "paddingLeft": "15px",
      "paddingRight": "15px",
      "paddingBottom": "15px",
      "backgroundColor": "#F8F4EC"
    }
  },
  "footer": {
    "container": {
      "display": "flex",
      "height": "60px",
      "backgroundColor": "#E7E6DD",
      "color": "#111111",
      "justifyContent": "center",
      "alignItems": "center"
    }
  }
}